{
    "name": "ethereum-mint-bot",
    "version": "1.0.0",
    "type": "module",
    "description": "A free bot for minting NFTs on the Ethereum network",
    "exports": "./dist/index.js",
    "types": "./dist/index.d.js",
    "files": [
        "dist"
    ],
    "repository": "https://github.com/frazierjoe/ethereum-mint-bot.git",
    "author": "Joe <frazierj@wustl.edu>",
